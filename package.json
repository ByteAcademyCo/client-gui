{
  "name": "edChain-gui",
  "version": "0.0.1",
  "description": "Desktop client for edChain",
  "repository": "edchainio/client-gui",
  "author": "William Mantly <wmantly@gmail.com>",
  "license": "GPL-3.0",
  "main": "app.js",
  "scripts": {
    "start": "electron app.js",
    "build": "electron-packager . app --platform linux --arch x64 --out dist/ --overwrite --ignore=electron-packager",
    "rpm64": "electron-installer-redhat --src dist/app-linux-x64/ --dest dist/installers/ --arch x86_64 --overwrite",
    "deb64": "electron-installer-debian --src dist/app-linux-x64/ --dest dist/installers/ --arch amd64 --overwrite"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-installer-debian": "^0.6.0",
    "electron-installer-dmg": "^0.2.1",
    "electron-log": "^2.2.11",
    "electron-packager": "^9.1.0"
  },
  "dependencies": {
    "electron-log": "^2.2.11",
    "electron-pubsub": "^0.2.2",
    "ipfs-api": "^15.0.1",
    "mustache": "^2.3.0"
  }
}
